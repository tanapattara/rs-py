  "version": "0.2.0",
  "configurations": [
    {
      "name": "Googleplace",
      "type": "python",
      "request": "launch",
      "program": "googleplace.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Debug Googleplace",
      "type": "python",
      "request": "launch",
      "program": "googleplace.py",
      "console": "internalConsole"
    },
    {
      "name": "Debug Googleplace force-update",
      "type": "python",
      "request": "launch",
      "program": "googleplace.py",
      "args": ["--force-update"],
      "console": "internalConsole"
    }
  ]
}
